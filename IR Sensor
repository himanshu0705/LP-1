int IRSensor = 8;
int LED = 13;

void setup()
{
    Serial.begib(9600);
    pinMode(IRSensor,INPUT);
    pinMode(LED,OUTPUT);
}

void loop()
{
    int status = digitalRead(IRSensor);
    if(status == 0)
    {
        digitalWrite(LED,HIGH);
        Serial.println("Object detected");
    }
    else
    {
        digitalWrite(LED,LOW);
        Serial.println("Object not detected");
    }
}
